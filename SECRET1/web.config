<configuration>
    <system.webServer>
        <security>
            <authorization>
                <add accessType="Deny" users="?" />
                <add accessType="Allow" roles="admin" />
            </authorization>
        </security>
    </system.webServer>
    <system.web>
        <authorization>
            <deny users="?" />
        </authorization>
    </system.web>
</configuration>
